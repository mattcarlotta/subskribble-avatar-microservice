{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "controllers": "./controllers",
          "db": "./database/db",
          "queries": "./database/queries",
          "env": "./env",
          "middlewares": "./middlewares",
          "routes": "./routes",
          "server": "./server",
          "strategies": "./services/strategies",
          "authErrors": "./shared/authErrors",
          "helpers": "./shared/helpers",
          "utils": "./utils"
        }
      }
    ]
  ]
}
